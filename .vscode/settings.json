{
    "rust-analyzer.cargo.features": [
        "player",
        "cpal-output",
        "scripting",
        "lua-jit"
    ],
    "Lua.runtime.version": "LuaJIT",
    "Lua.runtime.builtin": {
        "os": "disable",
        "io": "disable",
        "package": "disable",
        "debug": "disable",
        "bit": "disable",
        "jit": "disable",
        "ffi": "disable",
    },
    "Lua.workspace.library": [
        "types/pattrns"
    ],
    "Lua.workspace.ignoreDir": [
        "docs"
    ],
    "Lua.hint.arrayIndex": "Disable"
}