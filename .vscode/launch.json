  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug example 'play'",
      "cargo": {
        "args": [
          "build",
          "--example=play",
          "--features=player",
          "--package=afseq"
        ],
        "filter": {
          "name": "play",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug example 'play-script'",
      "cargo": {
        "args": [
          "build",
          "--example=play-script",
          "--features=player",
          "--package=afseq"
        ],
        "filter": {
          "name": "play-script",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "arg": [],
      "name": "Debug current Lua File",
      "program": "${file}",
      "request": "launch",
      "stopOnEntry": true,
      "type": "lua"
    },
  ]
}